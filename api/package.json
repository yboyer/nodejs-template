{
    "name": "api",
    "version": "0.0.0",
    "main": "src/index.js",
    "license": "MIT",
    "private": true,
    "engines": {
        "node": "22.21.1"
    },
    "scripts": {
        "build": "tsc",
        "ts-node:local": "ts-node -r tsconfig-paths/register",
        "dev": "nodemon --watch './src/**/*.ts' --exec ts-node -r tsconfig-paths/register ./src/index.ts",
        "eslint": "eslint src --ext ts",
        "lint": "npm run eslint -- --fix",
        "lintnofix": "npm run eslint",
        "ts:check": "tsc --noEmit",
        "format": "prettier \"./**/*.{js,jsx,ts,tsx,json}\" --write",
        "prisma:pull": "prisma db pull && prisma generate",
        "prisma:generate": "prisma generate",
        "prepare": "npm run prisma:generate",
        "preversion": "echo only on root && exit 1"
    },
    "lint-staged": {
        "*.{ts,tsx}": [
            "eslint --fix"
        ]
    },
    "_moduleAliases": {
        "@": "./src"
    },
    "devDependencies": {
        "@babel/preset-env": "^7.28.5",
        "@types/bcrypt": "^5.0.2",
        "@types/luxon": "^3.7.1",
        "@types/module-alias": "^2.0.4",
        "@types/node": "^22.19.1",
        "husky": "^8.0.3",
        "jest": "29.7.0",
        "lint-staged": "^13.3.0",
        "nodemon": "^3.1.11",
        "prisma": "^4.16.2",
        "ts-node": "^10.9.2",
        "tsconfig-paths": "^4.2.0",
        "typescript": "5.9.3"
    },
    "dependencies": {
        "@fastify/cors": "^11.2.0",
        "@fastify/helmet": "^11.1.1",
        "@fastify/swagger": "^8.15.0",
        "@fastify/swagger-ui": "^1.10.2",
        "@hapi/boom": "^10.0.1",
        "@keplr/typed-ajv": "^3.0.5",
        "@prisma/client": "^4.16.2",
        "bcrypt": "^5.1.1",
        "dotenv": "^16.6.1",
        "fastify": "^4.29.1",
        "fastify-file-upload": "^4.0.0",
        "form-data": "^4.0.5",
        "got": "11.8.6",
        "jose": "^4.15.9",
        "lodash": "^4.17.21",
        "luxon": "^3.7.2",
        "mailgun.js": "^8.2.2",
        "make-promises-safe": "^5.1.0",
        "module-alias": "^2.2.3",
        "nanoid": "^3.3.11",
        "pino-pretty": "^10.3.1"
    }
}
